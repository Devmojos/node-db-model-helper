{
  "name": "@devmojos/db-model-helper",
  "version": "0.0.1",
  "description": "get information from a database, show tables, columns, etc. Supports[ Mysql ]",
  "main": "index.js",
  "files": [
    "dist/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Devmojos/node-db-model-helper.git"
  },
  "scripts": {
    "test": "jest --config jestconfig.json",
    "local": "ts-node ./src/index.development.ts",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "keywords": [
    "database",
    "mysql",
    "structure",
    "model",
    "reverse",
    "show",
    "tables",
    "columns"
  ],
  "author": "info@devmojos.com",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^26.0.22",
    "jest": "^26.6.3",
    "jest-extended": "^0.11.5",
    "knex": "^0.95.4",
    "prettier": "^2.2.1",
    "testcontainers": "^7.7.0",
    "ts-jest": "^26.5.4",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "mysql2": "^2.2.5"
  }
}
